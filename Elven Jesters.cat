    <infoGroup id="1e06-691a-dcbd-b0d2" name="Psychic Spells" publicationId="0994-d14c-634b-7b1b" page="2" hidden="false">
      <profiles>
        <profile id="6c4a-aa09-c5ef-7b8e" name="(4+) Psychic Fog" publicationId="0994-d14c-634b-7b1b" page="2" hidden="false" typeId="0680-6f78-f88c-15fe" typeName="Psychic Spell">
          <characteristics>
            <characteristic name="Psychic Spell" typeId="34ee-63f1-d514-1028">Target enemy unit within 12&quot; gets -2 to its next morale test roll.</characteristic>
